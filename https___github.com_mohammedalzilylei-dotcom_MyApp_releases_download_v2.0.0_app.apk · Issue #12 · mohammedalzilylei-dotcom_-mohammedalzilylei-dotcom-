  0% { opacity: 0; }
  100% { opacity: 1; }
}

.anim-fade-out { animation-name: fade-out; animation-duration: 1s; animation-fill-mode: forwards; animation-timing-function: ease-out; }

.anim-fade-out.fast { animation-duration: 0.3s; }

@keyframes fade-out { 
  0% { opacity: 1; }
  100% { opacity: 0; }
}

